<html>

<body>


    <!-- Navigation -->
    


<!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Departments
                    
                </h1>
                <ol class="breadcrumb">
                    <li><a href="dcs.html">Home</a>
                    </li>
                    <li class="active">Departments</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <!-- Content Row -->
        <div class="row">
            <!-- Sidebar Column -->
            <div class="col-md-2">
                <div class="list-group">
                    <a href="index.html" class="list-group-item">Events</a>
                    <a href="about.html" class="list-group-item">Infrastructure</a>
                    <a href="services.html" class="list-group-item">Human Resource</a>
                    <a href="contact.html" class="list-group-item">Project</a>
                    <a href="correspondences.html" class="list-group-item">Correspondence</a>
                    <a href="portfolio-2-col.html" class="list-group-item">Training</a>
                    <a href="portfolio-3-col.html" class="list-group-item">Software Technical Support</a>
                    <a href="portfolio-4-col.html" class="list-group-item">Hardware and Networking Support</a>
                    <a href="portfolio-item.html" class="list-group-item">Future Enhancements</a>
                    
                </div>
            </div>
            <!-- Content Column -->
    
             <div class="col-md-9">
            <h1><b>Listing DC Members</b></h1><br>


            <table cellspacing='0'> <!-- cellspacing='0' is important, must stay -->
            <tr><th>First name</th><th>Mid name</th><th>Last name</th><th>Date of birth
            </th><th>Gender</th><th>Blood group</th><th>Local address</th><th>Permanent address</th><th>Father name</th><th>Mother name</th><th>Guardian name</th><th>Course name</th><th>Github link</th><th>Linkedin link</th><th>Sem of joining</th><th>Email</th><th></th><th></th><th>
            </th></tr><!-- Table Header -->
            
            <% @dc_members.each do |dc_member| %>
            
            <tr><td><%= dc_member.first_name %></td>
            <td><%= dc_member.mid_name %></td>
            <td><%= dc_member.last_name %></td>
            <td><%= dc_member.date_of_birth %></td>
            <td><%= dc_member.gender %></td>
            <td><%= dc_member.blood_group %></td>
            <td><%= dc_member.local_address %></td>
            <td><%= dc_member.permanent_address %></td>
            <td><%= dc_member.father_name %></td>
            <td><%= dc_member.mother_name %></td>
            <td><%= dc_member.guardian_name %></td>
            <td><%= dc_member.course_name %></td>
            <td><%= dc_member.github_link %></td>
            <td><%= dc_member.linkedin_link %></td>
            <td><%= dc_member.sem_of_joining %></td>
            <td><%= dc_member.email %></td>
            <td><%= link_to 'Show', dc_member %></td>
            <td><%= link_to 'Edit', edit_dc_member_path(dc_member) %></td>
            <td><%= link_to 'Destroy', dc_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr><!-- Table Row -->
            <% end %>
            </table>
        </br>
            <%= link_to 'New DC Member', new_dc_member_path %>
            </div>
        </div>
    </div>


</body>
</html>
