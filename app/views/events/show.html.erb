<html>    
<body>


    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header"><b>Event Details</b></h1>
                <ol class="breadcrumb">
                  <p id="notice"><%= notice %></p>

                   
            <p><b>Type of event :&nbsp;</b>
              <%= @event.type_of_event %></p>

            <br>

              <p><b>Organizer :&nbsp;</b>
              <%= @event.organizer_id %>
            </p>

            <br>
            <p>
              <b>Topic :&nbsp;</b>
              <%= @event.topic %>
            </p>
            <br>
            <p>
              <b>Venue :&nbsp;</b>
              <%= @event.venue %>
            </p>
            <br>

            <p>
              <b>Date :&nbsp;</b>
              <%= @event.date %>
            </p>
            <br>

            <p>
              <b>Time :&nbsp;</b>
              <%= @event.time %>
            </p>
            <br>

            <p>
              <b>Duration :&nbsp;</b>
              <%= @event.duration %>
            </p>
            <br>

            <p>
              <b>Material link :&nbsp;</b>
              <%= @event.material_link %>
            </p>
            <br>

            <p>
              <b>Summary link :&nbsp;</b>
              <%= @event.summary_link %>
            </p>
            <br>


            <%= link_to 'Edit', edit_event_path(@event) %> |
            <%= link_to 'Back', events_path %>
            </ol>
            </div>
            
        </div>
    </div>
</body>
</html>
              

