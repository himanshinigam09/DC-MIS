
<html xmlns="http://www.w3.org/1999/xhtml">




<body>

    

<!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12"><br/><br/><br/>
                <h1 class="page-header<br/><br/><br/>">Departments
                    
                </h1>
                <ol class="breadcrumb">
                    <li><a href="dcs.html">Home</a>
                    </li>
                    <li class="active">Departments</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <!-- Content Row -->
        <div class="row">
            <!-- Sidebar Column -->
            <div class="col-md-2">
                <div class="list-group">
                   <a href="events.html" class="list-group-item">Events</a>
                    <a href="infrastructures.html" class="list-group-item">Infrastructure</a>
                    <a href="profiles.html" class="list-group-item">Human Resource</a>
                    <a href="projects.html" class="list-group-item">Project</a>
                    <a href="correspondences.html" class="list-group-item">Correspondence</a>
                    <a href="publications.html" class="list-group-item">Publications</a>
                    
                </div>
            </div>
            <!-- Content Column -->


             <div class="col-md-9">
                <h1><b>Listing Publications</b></h1><br>

                <table cellspacing='0'>
                <tr>
                <th>Publication name</th>
                <th>Level</th>
                <th>Organizer name</th>
                <th>Sponsers name</th>
                <th>Location</th>
                <th>Paper</th>
                <th>Paper title</th>
                <th>Date of publication</th>
                <th>Abstract</th>
                <th>Link</th>
                <th>Author</th>
                <th></th>
                <th></th>
                <th></th>
                </tr>
                <% @publications.each do |publication| %>
                  <tr>
                    <td><%= publication.publication_name %></td>
                    <td><%= publication.level %></td>
                    <td><%= publication.organizer_name %></td>
                    <td><%= publication.sponsers_name %></td>
                    <td><%= publication.location %></td>
                    <td><%= publication.paper_id %></td>
                    <td><%= publication.paper_title %></td>
                    <td><%= publication.date_of_publication %></td>
                    <td><%= publication.abstract %></td>
                    <td><%= publication.link %></td>
                    <td><%= publication.author %></td>
                    <td><%= link_to 'Show', publication %></td>
                    <td><%= link_to 'Edit', edit_publication_path(publication) %></td>
                    <td><%= button_to 'Destroy', publication, method: :delete, :onclick=> "return confirm('Are you sure you want to delete this item?')" %></td>
                  </tr>
                
                <% end %>
                </table>

                <br />

                <%= link_to 'New Publication', new_publication_path %><br/>
                               
            </div>
        </div>
    </div>
</body>
</html>