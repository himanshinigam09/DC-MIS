<div class="col-sm-9 col-md-10 main " >
<h1 class="page-header">Listing Publications</h1>
<div class="panel-group" id="accordion" style="background-color:white;">
<div class="row placeholders" >
<div class="col-lg-11" >
<!--toggle sidebar button-->

<!--search box -->
<%= form_tag publications_path, :method => 'get' do %>  
      <p>  
        <%= text_field_tag :search, params[:search], :placeholder => 'Enter paper title' %>  
        <%= submit_tag "Search", :name => nil %>  
      </p>  
    <% end %> 
<table cellspacing='0'>
<tr>
<th>Paper title</th>
<th>Organizer name</th>
<th>Location</th>
<th>Publication name</th>
<th>Date of publication</th>
<th>Link</th>
<th>Author</th>
<th></th>
<th></th>
<th></th>
</tr>
<% @publications.each do |publication| %>
<tr>
<td><%= publication.paper_title %></td>
<td><%= publication.organizer_name %></td>
<td><%= publication.location %></td>
<td><%= publication.publication_name %></td>
<td><%= publication.date_of_publication %></td>
<td><%= publication.link %></td>
<td><%= publication.author %></td>
<td><%= button_to 'Show', publication,method: :get %></td>
<td><%= button_to 'Edit', edit_publication_path(publication),method: :get %></td>
<td><%= button_to 'Destroy', publication, method: :delete, :onclick=> "return confirm('Are you sure you want to delete this item?')" %></td>
</tr>
<% end %>
</table>
<br />
<%= button_to 'Add Publicaion', new_publication_path, method: :get, :class => 'btn btn-primary' %><br/>
<%= paginate @publications %>
</div>
</div>
</div>
</div>
</div>
</div>

</body>
