<html>
<body>

    

<!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Departments
                    
                </h1>
                <ol class="breadcrumb">
                    <li><a href="dcs.html">Home</a>
                    </li>
                    <li class="active">Departments</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <!-- Content Row -->
        <div class="row">
            <!-- Sidebar Column -->
            <div class="col-md-2">
                <div class="list-group">
                   <a href="events.html" class="list-group-item">Events</a>
                    <a href="infrastructures.html" class="list-group-item">Infrastructure</a>
                    <a href="profiles.html" class="list-group-item">Human Resource</a>
                    <a href="projects.html" class="list-group-item">Project</a>
                    <a href="correspondences.html" class="list-group-item">Correspondence</a>
                    
                </div>
            </div>
            <!-- Content Column -->


             <div class="col-md-9">
                <h1><b>Listing Projects</b></h1><br>

                <table cellspacing='0'>
                <tr>
                <th>Project name</th>
                <th>Project type</th>
                <th>Project description</th>
                <th>Project leader</th>
                <th>Team</th>
                <th>Project status</th>
                <th>Dc page link</th>
                <th>Github page link</th>
                <th></th>
                <th></th>
                <th></th>
                </tr>

                <% @projects.each do |project| %>
                <tr>
                <td><%= project.project_name %></td>
                <td><%= project.project_type %></td>
                <td><%= project.project_description %></td>
                <td><%= project.project_leader %></td>
                <td><%= project.team_id %></td>
                <td><%= project.project_status %></td>
                <td><%= project.dc_page_link %></td>
                <td><%= project.github_page_link %></td>
                <td><%= link_to 'Show', project %></td>
                <td><%= link_to 'Edit', edit_project_path(project) %></td>
                <td><%= button_to 'Destroy', project , method: :delete, class: :destroy%></td>
                </tr>
                <% end %>
                </table>

                <br />

                <%= link_to 'New Project', new_project_path %>
            </div>
        </div>
    </div>
</body>
</html>