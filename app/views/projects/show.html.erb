   </script>
    <link rel="stylesheet" type="text/css" href="bootstrap/css/bootstrap1.min.css" />
    <link rel="stylesheet" type="text/css" href="font-awesome/css/font-awesome.min.css" />
</head>     
<body>


    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header"><b>Project Details</b></h1>
                <ol class="breadcrumb">

        <p>
          <b>Project name :&nbsp;</b>
          <%= @project.project_name %>
        </p>
        <br>

        <p>
          <b>Project type :&nbsp;</b>
          <%= @project.project_type %>
        </p>
        <br>

        <p>
          <b>Project description :&nbsp;</b>
          <%= simple_format(@project.project_description) %>
        </p>
        <br>

        <p>
          <b>Project leader :&nbsp;</b>
          <%= @project.project_leader %>
        </p>
        <br>

        <p>
          <b>Team :&nbsp;</b>
          <%= @project.team_id %>
        </p>
        <br>

        <p>
          <b>Project status :&nbsp;</b>
          <%= @project.project_status %>
        </p>
        <br>

        <p>
          <b>Dc page link :&nbsp;</b>
          <%= @project.dc_page_link %>
        </p>
        <br>

        <p>
          <b>Github page link :&nbsp;</b>
          <%= @project.github_page_link %>
        </p>

        <br>
        <%= link_to 'Edit', edit_project_path(@project) %> |
        <%= link_to 'Back', projects_path %>
        </ol>
        </div>
        </div>
    </div>
    
</body>
</html>
